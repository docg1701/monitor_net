{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "NetMonitor",
  "version": "1.0.4",
  "identifier": "com.galvani.netmonitor",
  "build": {
    "frontendDist": "../www/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "npm start",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "NetMonitor",
        "width": 1004,
        "height": 627,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.png"
    ]
  }
}
